
if [ -z "${1}" ]; then echo error; exit 1; fi

current=${1}.oc
exp=.expected/

a=${current}.log
aa=${exp}${a}
diff ${a} ${aa}

xxd="xxd -c 48 -g 4 "

comp () {
	echo ${1}
	b=${1}
	${xxd}${b} > 1
	bb=${exp}${b}
	bbb=${bb}.txt
	base64 -d ${bbb} > 2
	${xxd}2 > 3
	diff 1 3
	rm 1 2 3
}

comp ${1}.o
comp ${current}.x
